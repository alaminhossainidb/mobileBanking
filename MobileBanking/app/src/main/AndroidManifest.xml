<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobilebanking">

    <application
        android:allowBackup="true"
        android:icon="@drawable/coins_round"
        android:label="@string/app_name"
        android:roundIcon="@drawable/coins"
        android:supportsRtl="true"
        android:theme="@style/Theme.MobileBanking">
        <activity
            android:name=".SuccessActivity"
            android:exported="true"
            android:label="@string/success"/>
        <activity
            android:name=".AddMoneyActivity"
            android:exported="true"
            android:label="@string/add"/>
        <activity
            android:name=".ConfirmActivity"
            android:exported="true"
            android:label="@string/confirm"/>
        <activity
            android:name=".SendMoney"
            android:exported="true"
            android:label="@string/send"/>
        <activity
            android:name=".ui.login.LoginActivity"
            android:exported="true"
            android:label="@string/title_activity_login">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name">



        </activity>
    </application>

</manifest>